extends layout

block content
    .page-header
        h1 Beijing Air Quality Index
    if doc.num < 50 
        h1(style='color:green;font-size:38px;') #{doc.str}
    else if doc.num < 100
        h1(style='color:orange;font-size:38px;') #{doc.str}
    else 
        h1(style='color:red;font-size:38px;') #{doc.str}
    h1 pagetime = #{doc.pagetime}
    h1 server fetch time = #{doc.time}
